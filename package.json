{
  "private": true,
  "name": "mehuge-ui",
  "version": "0.1.0",
  "description": "Mehuge's Camelot Unchained UIs",
  "license": "MPL-2.0",
  "main": "dist/index.html",
  "homepage": "https://github.com/mehuge/mehuge-uis",
  "repository": {
    "type": "git",
    "url": "https://github.com/mehuge/mehuge-uis.git"
  },
  "modules": {},
  "scripts": {
    "start": "nps",
    "setup": "npm i && npm start build",
    "clean.all": "rimraf build dist node_modules || echo 'already cleaned'"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "browserify": {
    "transform": [
      "aliasify"
    ]
  },
  "aliasify": "./aliasify.config.js",
  "dependencies": {
    "core-js": "^2.5.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.3"
  },
  "devDependencies": {
    "@csegames/camelot-unchained": "^0.1.47",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "aliasify": "^2.1.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^8.0.0",
    "browserify": "^13.0.0",
    "browserify-casesensitiverequire": "^1.0.1",
    "copyfiles": "^1.0.0",
    "envify": "^4.0.0",
    "livereload": "^0.7.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "nps": "^5.9.2",
    "nps-utils": "^1.6.0",
    "rimraf": "^2.6.1",
    "sass-importer-node": "^1.0.1",
    "tslint": "^5.4.3",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "^2.9.1",
    "typings": "^1.3.2",
    "watch-cli": "^0.2.3"
  }
}
